{
  "usesTypeScript": true,
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": false,
      "withTest": true,
      "withStory": true,
      "withIndex": true,
      "withLazy": false,
      "flat": false,
      "customTemplates": {
        "index": "configuration/react_templates/component/index.tsx",
        "component": "configuration/react_templates/component/TemplateName.tsx",
        "test": "configuration/react_templates/component/TemplateName.test.tsx",
        "story": "configuration/react_templates/component/TemplateName.stories.tsx"
      }
    },
    "page": {
      "path": "src/pages",
      "withStyle": false,
      "withTest": true,
      "withStory": true,
      "withIndex": true,
      "withLazy": false,
      "flat": false,
      "customTemplates": {
        "index": "configuration/react_templates/page/index.tsx",
        "component": "configuration/react_templates/page/TemplateName.tsx",
        "test": "configuration/react_templates/page/TemplateName.test.tsx",
        "story": "configuration/react_templates/page/TemplateName.stories.tsx"
      }
    },
    "util": {
      "path": "src/utils",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": true,
      "flat": true
    }
  },
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "usesStyledComponents": true
}